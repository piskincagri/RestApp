@model List<ResultAboutDto>
@foreach (var item in Model)
{
<section class="about_section layout_padding">
    <div class="container  ">

        <div class="row">
            <div class="col-md-6 ">
                <div class="img-box">
                    <img src="@item.ImageUrl" alt="">
                </div>
            </div>
            <div class="col-md-6">
                <div class="detail-box">
                    <div class="heading_container">
                        <h2>
                            @item.Title
                        </h2>
                    </div>
                    <p>
                        @item.Description
                    </p>
                    <p>
                        @item.Description1
                    </p>
                    <p>
                        @item.Description2
                    </p>
                    <p>
                        @item.Description3
                    </p>
                    <p>
                        @item.Description4
                    </p>
                    <p>
                        @item.Description5
                    </p>

                    <a href="/Menu/Index/">
                        Hemen Sipariş Verin
                    </a>
                </div>
            </div>
        </div>
    </div>
</section>
}